# Discord Bot Configuration
# You can use either DISCORD_TOKEN or DISCORD_BOT_TOKEN
DISCORD_TOKEN=your_bot_token_here
# DISCORD_BOT_TOKEN=your_bot_token_here  # Alternative variable name
DISCORD_CLIENT_ID=your_client_id_here
DISCORD_GUILD_ID=your_test_guild_id_here

# Environment
NODE_ENV=development

# Bot Settings
COMMAND_PREFIX=!
OWNER_ID=your_discord_user_id

# Module Settings
SUSLINK_AUTO_SCAN=true
TRUST_THRESHOLD=60

# Dashboard Integration (optional)
# Point to your running dashboard service
DASHBOARD_URL=http://localhost:5055
