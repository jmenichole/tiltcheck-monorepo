{
  "$schema": "https://railway.app/railway.schema.json",
  "build": {
    "builder": "dockerfile",
    "dockerfile": "./backend/Dockerfile"
  },
  "deploy": {
    "startCommand": "cd backend && node dist/index.js",
    "healthcheckPath": "/health",
    "healthcheckInterval": 10,
    "restartPolicyMaxRetries": 5
  }
}
