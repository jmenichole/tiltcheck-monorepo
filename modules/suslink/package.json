{
  "name": "@tiltcheck/suslink",
  "version": "0.1.0",
  "private": true,
  "description": "AI-powered link scanner and risk detector for TiltCheck",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./suslink": {
      "types": "./dist/suslink.d.ts",
      "import": "./dist/suslink.js",
      "default": "./dist/suslink.js"
    },
    "./scanner": {
      "types": "./dist/scanner.d.ts",
      "import": "./dist/scanner.js",
      "default": "./dist/scanner.js"
    },
    "./trust-domain": {
      "types": "./dist/trust-domain.d.ts",
      "import": "./dist/trust-domain.js",
      "default": "./dist/trust-domain.js"
    }
  },
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "clean": "rm -rf dist",
    "test": "echo \"Tests coming soon\""
  },
  "dependencies": {
    "@tiltcheck/types": "workspace:*",
    "@tiltcheck/event-router": "workspace:*",
    "@tiltcheck/config": "workspace:*"
  },
  "devDependencies": {
    "@types/node": "^24.10.1",
    "tsx": "^4.7.0",
    "typescript": "^5.3.0"
  }
}
