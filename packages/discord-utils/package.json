{
  "name": "@tiltcheck/discord-utils",
  "version": "0.1.0",
  "description": "Shared Discord utilities for TiltCheck",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./embeds": {
      "types": "./dist/embeds.d.ts",
      "import": "./dist/embeds.js",
      "default": "./dist/embeds.js"
    },
    "./formatters": {
      "types": "./dist/formatters.d.ts",
      "import": "./dist/formatters.js",
      "default": "./dist/formatters.js"
    },
    "./validators": {
      "types": "./dist/validators.d.ts",
      "import": "./dist/validators.js",
      "default": "./dist/validators.js"
    },
    "./trust-adapter": {
      "types": "./dist/trust-adapter.d.ts",
      "import": "./dist/trust-adapter.js",
      "default": "./dist/trust-adapter.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@tiltcheck/types": "workspace:*",
    "@tiltcheck/event-router": "workspace:*",
    "discord.js": "^14.14.1"
  },
  "devDependencies": {
    "@types/node": "^24.10.1",
    "typescript": "^5.3.0"
  }
}
