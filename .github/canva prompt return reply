    }

    @keyframes chipDissolve {
      0% {
        opacity: 0;
        transform: scale(1) translateY(0);
        filter: blur(0px);
      }
      25% {
        opacity: 0.6;
      }
      50% {
        opacity: 0.4;
        transform: scale(1.2) translateY(-20px);
      }
      75% {
        opacity: 0.2;
        transform: scale(1.4) translateY(-40px);
        filter: blur(2px);
      }
      100% {
        opacity: 0;
        transform: scale(1.6) translateY(-60px);
        filter: blur(4px);
      }
    }

    /* Data stream particles */
    .data-stream {
      position: absolute;
      top: 0;
      left: 0;
      width: 100%;
      height: 100%;
      z-index: 3;
    }

    .particle {
      position: absolute;
      width: 2px;
      height: 2px;
      background: rgba(0, 255, 255, 0.8);
      box-shadow: 0 0 4px rgba(0, 255, 255, 0.6);
      animation: particleFlow 3s linear infinite;
    }

    @keyframes particleFlow {
      0% {
        opacity: 0;
        transform: translateY(0) translateX(0);
      }
      10% {
        opacity: 1;
      }
      90% {
        opacity: 0.8;
      }
      100% {
        opacity: 0;
