    function renderIconGrid() {
      const grid = document.getElementById('iconGrid');
      grid.innerHTML = '';
      
      Object.entries(iconSet).forEach(([key, data]) => {
        const svg = activeVariant === 'solid' ? data.solid : data.outline;
        
        const card = document.createElement('div');
        card.className = 'icon-card';
        card.innerHTML = `
          <div class="icon-display">${svg}</div>
          <h3 class="icon-title">${data.title}</h3>
          <p class="icon-subtitle">${data.subtitle}</p>
          <div class="download-btns">
            <button class="dl-btn" onclick="downloadSingle('${key}', 'svg')">SVG</button>
            <button class="dl-btn" onclick="downloadSingle('${key}', '512')">512px</button>
            <button class="dl-btn" onclick="downloadSingle('${key}', '128')">128px</button>
          </div>
        `;
        grid.appendChild(card);
      });
    }

    function setVariant(variant) {
      activeVariant = variant;
      document.querySelectorAll('.variant-btn').forEach(btn => {
        btn.classList.remove('active');
      });
      event.target.classList.add('active');
      renderIconGrid();
    }

    function notify(msg) {
      const toast = document.getElementById('toast');
      toast.textContent = msg;
      toast.classList.add('visible');
      setTimeout(() => {
        toast.classList.remove('visible');
      }, 2800);
    }

    function downloadSingle(key, format) {
      const data = iconSet[key];
      const svg = activeVariant === 'solid' ? data.solid : data.outline;
      
      if (format === 'svg') {
        const blob = new Blob([svg], { type: 'image/svg+xml' });
        const url = URL.createObjectURL(blob);
        const link = document.createElement('a');
        link.href = url;
        link.download = `${key}_${activeVariant}.svg`;
        link.click();
        URL.revokeObjectURL(url);
        notify(`${data.title} SVG downloaded ✓`);
      } else {
        const size = parseInt(format);
        const canvas = document.createElement('canvas');
        canvas.width = size;
        canvas.height = size;
        const ctx = canvas.getContext('2d');
        
        const image = new Image();
        image.onload = () => {
          ctx.fillStyle = '#1a1a1a';
          ctx.fillRect(0, 0, size, size);
          ctx.drawImage(image, 0, 0, size, size);
          
          canvas.toBlob(blob => {
            const url = URL.createObjectURL(blob);
            const link = document.createElement('a');
            link.href = url;
            link.download = `${key}_${activeVariant}_${size}x${size}.png`;
            link.click();
            URL.revokeObjectURL(url);
            notify(`${data.title} ${size}×${size} downloaded ✓`);
          });
        };
        image.src = 'data:image/svg+xml;base64,' + btoa(svg);
      }
    }

    function bulkExport(format) {
      let count = 0;
      const total = Object.keys(iconSet).length;
      
      Object.keys(iconSet).forEach((key, idx) => {
        setTimeout(() => {
          downloadSingle(key, format);
          count++;
          if (count === total) {
            setTimeout(() => {
              const label = format === 'svg' ? 'SVG' : `${format}×${format}`;
              notify(`All ${total} icons exported as ${label} ✓`);
            }, 600);
          }
        }, idx * 250);
      });
    }

    const defaultConfig = {
      set_title: "Dashboard Icon Set",
      description: "Professional minimal line icons • Cyan on Charcoal • 2px stroke"
    };

    async function onConfigChange(config) {
      document.getElementById('main-title').textContent = config.set_title || defaultConfig.set_title;
      document.getElementById('main-description').textContent = config.description || defaultConfig.description;
    }

    if (window.elementSdk) {
      window.elementSdk.init({
        defaultConfig,
        onConfigChange,
        mapToCapabilities: () => ({
          recolorables: [],
          borderables: [],
          fontEditable: undefined,
          fontSizeable: undefined
        }),
        mapToEditPanelValues: (config) => new Map([
          ["set_title", config.set_title || defaultConfig.set_title],
          ["description", config.description || defaultConfig.description]
        ])
      });
    }

    renderIconGrid();
  </script>
