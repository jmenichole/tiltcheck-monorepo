          <rect x="84" y="96" width="24" height="16" rx="3" stroke="#00d9ff" stroke-width="2.5" fill="none"/>
        </svg>`
      },
      games: {
        name: "Games",
        label: "Gaming & Play",
        solid: `<svg viewBox="0 0 128 128" width="96" height="96">
          <path d="M36 48 L36 64 L52 64 L52 80 L68 80 L68 64 L92 64 L92 48 Q92 40 84 40 L44 40 Q36 40 36 48 Z" stroke="#00d9ff" stroke-width="2.5" stroke-linejoin="round" fill="none"/>
          <circle cx="76" cy="56" r="4" fill="#00d9ff"/>
          <circle cx="84" cy="72" r="4" fill="#00d9ff"/>
          <path d="M44 56 L48 56" stroke="#00d9ff" stroke-width="2.5" stroke-linecap="round" fill="none"/>
          <path d="M46 54 L46 58" stroke="#00d9ff" stroke-width="2.5" stroke-linecap="round" fill="none"/>
          <path d="M28 80 Q32 88 40 88 L88 88 Q96 88 100 80" stroke="#00d9ff" stroke-width="2.5" stroke-linecap="round" fill="none"/>
        </svg>`,
        outline: `<svg viewBox="0 0 128 128" width="96" height="96">
          <path d="M36 48 L36 64 L52 64 L52 80 L68 80 L68 64 L92 64 L92 48 Q92 40 84 40 L44 40 Q36 40 36 48 Z" stroke="#00d9ff" stroke-width="2.5" stroke-linejoin="round" fill="none"/>
          <circle cx="76" cy="56" r="4" stroke="#00d9ff" stroke-width="2" fill="none"/>
          <circle cx="84" cy="72" r="4" stroke="#00d9ff" stroke-width="2" fill="none"/>
          <path d="M44 56 L48 56" stroke="#00d9ff" stroke-width="2.5" stroke-linecap="round" fill="none"/>
          <path d="M46 54 L46 58" stroke="#00d9ff" stroke-width="2.5" stroke-linecap="round" fill="none"/>
          <path d="M28 80 Q32 88 40 88 L88 88 Q96 88 100 80" stroke="#00d9ff" stroke-width="2.5" stroke-linecap="round" fill="none"/>
        </svg>`
      }
    };

    function renderIcons() {
      const grid = document.getElementById('icon-grid');
      grid.innerHTML = '';
      
      Object.entries(icons).forEach(([key, icon]) => {
        const card = document.createElement('div');
        card.className = 'icon-card';
        
        const svg = currentVariant === 'solid' ? icon.solid : icon.outline;
        
        card.innerHTML = `
          <div class="icon-wrapper">${svg}</div>
          <div class="icon-name">${icon.name}</div>
          <div class="icon-label">${icon.label}</div>
          <div class="button-group">
            <button onclick="downloadIcon('${key}', 'svg')">SVG</button>
            <button onclick="downloadIcon('${key}', '512')">512px</button>
            <button onclick="downloadIcon('${key}', '128')">128px</button>
          </div>
        `;
        
        grid.appendChild(card);
      });
    }

    function switchVariant(variant) {
      currentVariant = variant;
      document.querySelectorAll('.variant-btn').forEach(btn => {
        btn.classList.remove('active');
      });
      event.target.classList.add('active');
      renderIcons();
    }

    function showToast(message) {
      const toast = document.getElementById('toast');
      toast.textContent = message;
      toast.classList.add('show');
      setTimeout(() => {
        toast.classList.remove('show');
      }, 2500);
    }

    function downloadIcon(iconKey, format) {
      const icon = icons[iconKey];
      const svg = currentVariant === 'solid' ? icon.solid : icon.outline;
      
      if (format === 'svg') {
        const blob = new Blob([svg], { type: 'image/svg+xml' });
        const url = URL.createObjectURL(blob);
        const a = document.createElement('a');
        a.href = url;
        a.download = `${iconKey}_${currentVariant}.svg`;
        a.click();
        URL.revokeObjectURL(url);
        showToast(`${icon.name} SVG downloaded`);
      } else {
        const size = parseInt(format);
        const canvas = document.createElement('canvas');
        canvas.width = size;
        canvas.height = size;
        const ctx = canvas.getContext('2d');
        
        const img = new Image();
        img.onload = function() {
          ctx.fillStyle = '#1a1a1a';
          ctx.fillRect(0, 0, size, size);
          ctx.drawImage(img, 0, 0, size, size);
          
          canvas.toBlob(function(blob) {
            const url = URL.createObjectURL(blob);
            const a = document.createElement('a');
            a.href = url;
            a.download = `${iconKey}_${currentVariant}_${size}x${size}.png`;
            a.click();
            URL.revokeObjectURL(url);
            showToast(`${icon.name} ${size}×${size} downloaded`);
          });
        };
        img.src = 'data:image/svg+xml;base64,' + btoa(svg);
      }
    }

    function exportAll(format) {
      let count = 0;
      Object.keys(icons).forEach((key, index) => {
        setTimeout(() => {
          downloadIcon(key, format);
          count++;
          if (count === Object.keys(icons).length) {
            setTimeout(() => {
              showToast(`All ${format === 'svg' ? 'SVG' : format + '×' + format} files downloaded`);
            }, 500);
          }
        }, index * 200);
      });
    }

    async function onConfigChange(config) {
      const title = config.set_title || defaultConfig.set_title;
      const description = config.description || defaultConfig.description;
      
      document.getElementById('main-title').textContent = title;
      document.getElementById('main-description').textContent = description;
    }

    if (window.elementSdk) {
      window.elementSdk.init({
        defaultConfig,
        onConfigChange,
        mapToCapabilities: (config) => ({
          recolorables: [],
          borderables: [],
          fontEditable: undefined,
          fontSizeable: undefined
        }),
        mapToEditPanelValues: (config) => new Map([
          ["set_title", config.set_title || defaultConfig.set_title],
          ["description", config.description || defaultConfig.description]
        ])
      });
    }

    renderIcons();
  </script>
