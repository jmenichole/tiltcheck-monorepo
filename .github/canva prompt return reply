  <script>
    const defaultConfig = {
      background_color: "#1a1a1f",
      accent_cyan: "#00ffff",
      accent_purple: "#9333ea",
      glow_intensity: 0.15
    };

    let config = { ...defaultConfig };

    // Generate data stream particles
    function generateParticles() {
      const container = document.getElementById('dataStream');
      const particleCount = 40;

      for (let i = 0; i < particleCount; i++) {
        const particle = document.createElement('div');
        particle.className = 'particle';
        particle.style.left = `${45 + Math.random() * 10}%`;
        particle.style.top = `${40 + Math.random() * 20}%`;
        particle.style.animationDelay = `${Math.random() * 3}s`;
        particle.style.setProperty('--drift', `${(Math.random() - 0.5) * 40}px`);
        
        if (Math.random() > 0.5) {
          particle.style.background = 'rgba(147, 51, 234, 0.7)';
          particle.style.boxShadow = '0 0 4px rgba(147, 51, 234, 0.5)';
        }
        
        container.appendChild(particle);
      }
    }

    // Generate point cloud lines
    function generateCloudLines() {
      const container = document.getElementById('pointCloud');
      const lineCount = 8;

      for (let i = 0; i < lineCount; i++) {
        const line = document.createElement('div');
        line.className = 'cloud-line';
        line.style.top = `${35 + Math.random() * 30}%`;
        line.style.width = `${150 + Math.random() * 200}px`;
        line.style.animationDelay = `${Math.random() * 5}s`;
        line.style.animationDuration = `${4 + Math.random() * 3}s`;
        
        if (Math.random() > 0.5) {
          line.style.background = 'linear-gradient(90deg, transparent, rgba(147, 51, 234, 0.35), transparent)';
        }
        
        container.appendChild(line);
      }
    }

    async function onConfigChange(newConfig) {
      const heroContainer = document.querySelector('.hero-container');
      const backgroundColor = newConfig.background_color || defaultConfig.background_color;
      
      heroContainer.style.background = `linear-gradient(135deg, ${backgroundColor} 0%, #0d0d12 50%, ${backgroundColor} 100%)`;
    }

    if (window.elementSdk) {
      window.elementSdk.init({
        defaultConfig,
        onConfigChange,
        mapToCapabilities: (cfg) => ({
          recolorables: [
            {
              get: () => cfg.background_color || defaultConfig.background_color,
              set: (value) => {
                cfg.background_color = value;
                window.elementSdk.setConfig({ background_color: value });
              }
            },
            {
              get: () => cfg.accent_cyan || defaultConfig.accent_cyan,
              set: (value) => {
                cfg.accent_cyan = value;
                window.elementSdk.setConfig({ accent_cyan: value });
              }
            },
            {
              get: () => cfg.accent_purple || defaultConfig.accent_purple,
              set: (value) => {
                cfg.accent_purple = value;
                window.elementSdk.setConfig({ accent_purple: value });
              }
            }
          ],
          borderables: [],
          fontEditable: undefined,
          fontSizeable: undefined
        }),
        mapToEditPanelValues: (cfg) => new Map()
      });
      
      config = window.elementSdk.config;
    }

    // Initialize animations
    generateParticles();
    generateCloudLines();
  </script>
