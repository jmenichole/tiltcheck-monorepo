  <script>
    const defaultConfig = {
      primary_color: "#0f0f14",
      cyan_accent: "#06b6d4",
      purple_accent: "#a855f7"
    };

    let config = { ...defaultConfig };

    // Generate floating data points
    function generateDataPoints() {
      const container = document.getElementById('dataPoints');
      const pointCount = 30;

      for (let i = 0; i < pointCount; i++) {
        const point = document.createElement('div');
        point.className = 'data-point';
        point.style.left = `${42 + Math.random() * 16}%`;
        point.style.top = `${35 + Math.random() * 30}%`;
        point.style.animationDelay = `${Math.random() * 4}s`;
        point.style.animationDuration = `${3 + Math.random() * 2}s`;
        point.style.setProperty('--drift-x', `${(Math.random() - 0.5) * 60}px`);
        point.style.setProperty('--drift-y', `${-20 - Math.random() * 40}px`);

        if (Math.random() > 0.6) {
          point.style.background = 'rgba(168, 85, 247, 0.9)';
          point.style.boxShadow = '0 0 6px rgba(168, 85, 247, 0.8)';
        }

        container.appendChild(point);
      }
    }

    async function onConfigChange(newConfig) {
      const heroContainer = document.querySelector('.hero-container');
      const primaryColor = newConfig.primary_color || defaultConfig.primary_color;
      
      heroContainer.style.background = `radial-gradient(ellipse at center, #1a1a24 0%, ${primaryColor} 100%)`;
    }

    if (window.elementSdk) {
      window.elementSdk.init({
        defaultConfig,
        onConfigChange,
        mapToCapabilities: (cfg) => ({
          recolorables: [
            {
              get: () => cfg.primary_color || defaultConfig.primary_color,
              set: (value) => {
                cfg.primary_color = value;
                window.elementSdk.setConfig({ primary_color: value });
              }
            },
            {
              get: () => cfg.cyan_accent || defaultConfig.cyan_accent,
              set: (value) => {
                cfg.cyan_accent = value;
                window.elementSdk.setConfig({ cyan_accent: value });
              }
            },
            {
              get: () => cfg.purple_accent || defaultConfig.purple_accent,
              set: (value) => {
                cfg.purple_accent = value;
                window.elementSdk.setConfig({ purple_accent: value });
              }
            }
          ],
          borderables: [],
          fontEditable: undefined,
          fontSizeable: undefined
        }),
        mapToEditPanelValues: () => new Map()
      });
      
      config = window.elementSdk.config;
    }

    // Initialize
    generateDataPoints();
  </script>
