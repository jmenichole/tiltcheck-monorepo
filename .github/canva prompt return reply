  <div class="hero-container"><!-- Radial glow center -->
   <div class="radial-glow"></div><!-- Grid layer -->
   <svg class="grid-layer" xmlns="http://www.w3.org/2000/svg"><defs>
     <pattern id="grid" width="40" height="40" patternunits="userSpaceOnUse">
      <path d="M 40 0 L 0 0 0 40" fill="none" stroke="rgba(0, 255, 255, 0.1)" stroke-width="0.5" />
     </pattern>
    </defs> <rect width="100%" height="100%" fill="url(#grid)" />
   </svg><!-- Dissolving chips -->
   <div class="chip chip-1"></div>
   <div class="chip chip-2"></div>
   <div class="chip chip-3"></div><!-- Data stream particles -->
   <div class="data-stream" id="dataStream"></div><!-- Trust gauge rings -->
   <div class="gauge-container">
    <div class="gauge-ring gauge-ring-1"></div>
    <div class="gauge-ring gauge-ring-2"></div>
   </div><!-- Point cloud lines -->
   <div class="point-cloud" id="pointCloud"></div><!-- Vertical accent lines -->
   <div class="vertical-accent accent-left"></div>
   <div class="vertical-accent accent-right"></div><!-- Edge highlights -->
   <div class="edge-highlight edge-top-left"></div>
   <div class="edge-highlight edge-bottom-right"></div><!-- Scanlines -->
   <div class="scanlines"></div><!-- Noise overlay -->
   <div class="noise-layer"></div>
