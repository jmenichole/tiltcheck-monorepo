  <script>
    const defaultConfig = {
      brand_name: "TiltCheck",
      primary_color: "#00ffff",
      secondary_color: "#8a2be2",
      background_start: "#0a0a1a",
      background_end: "#1a1a2e"
    };

    async function onConfigChange(config) {
      const brandName = config.brand_name || defaultConfig.brand_name;
      const primaryColor = config.primary_color || defaultConfig.primary_color;
      const secondaryColor = config.secondary_color || defaultConfig.secondary_color;
      const backgroundStart = config.background_start || defaultConfig.background_start;
      const backgroundEnd = config.background_end || defaultConfig.background_end;

      // Update brand name
      const brandElement = document.getElementById('brandName');
      if (brandElement) {
        brandElement.textContent = brandName;
      }

      // Update colors
      document.body.style.background = `linear-gradient(135deg, ${backgroundStart} 0%, ${backgroundEnd} 100%)`;
      
      const logoSvg = document.querySelector('.logo-svg');
      if (logoSvg) {
        logoSvg.style.filter = `drop-shadow(0 0 20px ${primaryColor}80)`;
        
        // Update all cyan strokes
        const cyanElements = logoSvg.querySelectorAll('[stroke="#00ffff"]');
        cyanElements.forEach(el => el.setAttribute('stroke', primaryColor));
        
        // Update all purple strokes
        const purpleElements = logoSvg.querySelectorAll('[stroke="#8a2be2"]');
        purpleElements.forEach(el => el.setAttribute('stroke', secondaryColor));
        
        // Update fills
        const cyanFills = logoSvg.querySelectorAll('[fill="#00ffff"]');
        cyanFills.forEach(el => el.setAttribute('fill', primaryColor));
      }

      // Update text gradient
      if (brandElement) {
        brandElement.style.background = `linear-gradient(135deg, ${primaryColor} 0%, ${primaryColor}cc 100%)`;
        brandElement.style.webkitBackgroundClip = 'text';
        brandElement.style.backgroundClip = 'text';
        brandElement.style.textShadow = `0 0 30px ${primaryColor}4d`;
      }
    }

    function mapToCapabilities(config) {
      return {
        recolorables: [
          {
            get: () => config.background_start || defaultConfig.background_start,
            set: (value) => {
              config.background_start = value;
              window.elementSdk.setConfig({ background_start: value });
            }
          },
          {
            get: () => config.background_end || defaultConfig.background_end,
            set: (value) => {
              config.background_end = value;
              window.elementSdk.setConfig({ background_end: value });
            }
          },
          {
            get: () => config.primary_color || defaultConfig.primary_color,
            set: (value) => {
              config.primary_color = value;
              window.elementSdk.setConfig({ primary_color: value });
            }
          },
          {
            get: () => config.secondary_color || defaultConfig.secondary_color,
            set: (value) => {
              config.secondary_color = value;
              window.elementSdk.setConfig({ secondary_color: value });
            }
          }
        ],
        borderables: [],
        fontEditable: undefined,
        fontSizeable: undefined
      };
    }

    function mapToEditPanelValues(config) {
      return new Map([
        ["brand_name", config.brand_name || defaultConfig.brand_name]
      ]);
    }

    // Initialize SDK
    if (window.elementSdk) {
      window.elementSdk.init({
        defaultConfig,
        onConfigChange,
        mapToCapabilities,
        mapToEditPanelValues
      });
    }
  </script>
