  <script>
    const defaultConfig = {
      page_title: "Degen Trust Badges",
      background_color: "#0a0e27",
      primary_color: "#00f5ff",
      secondary_color: "#a855f7",
      text_color: "#ffffff",
      font_family: "Courier New"
    };

    let config = { ...defaultConfig };

    async function onConfigChange(newConfig) {
      config = { ...config, ...newConfig };
      
      const pageTitle = document.getElementById('pageTitle');
      if (pageTitle) {
        pageTitle.textContent = config.page_title || defaultConfig.page_title;
      }

      document.body.style.background = `linear-gradient(135deg, ${config.background_color || defaultConfig.background_color} 0%, #1a1a2e 100%)`;
      
      const title = document.querySelector('.title');
      if (title) {
        title.style.background = `linear-gradient(90deg, ${config.primary_color || defaultConfig.primary_color}, ${config.secondary_color || defaultConfig.secondary_color})`;
        title.style.webkitBackgroundClip = 'text';
        title.style.backgroundClip = 'text';
        title.style.fontFamily = `${config.font_family || defaultConfig.font_family}, monospace`;
      }
    }

    function mapToCapabilities(config) {
      return {
        recolorables: [
          {
            get: () => config.background_color || defaultConfig.background_color,
            set: (value) => {
              config.background_color = value;
              window.elementSdk.setConfig({ background_color: value });
            }
          },
          {
            get: () => config.primary_color || defaultConfig.primary_color,
            set: (value) => {
              config.primary_color = value;
              window.elementSdk.setConfig({ primary_color: value });
            }
          },
          {
            get: () => config.secondary_color || defaultConfig.secondary_color,
            set: (value) => {
              config.secondary_color = value;
              window.elementSdk.setConfig({ secondary_color: value });
            }
          }
        ],
        borderables: [],
        fontEditable: {
          get: () => config.font_family || defaultConfig.font_family,
          set: (value) => {
            config.font_family = value;
            window.elementSdk.setConfig({ font_family: value });
          }
        },
        fontSizeable: undefined
      };
    }

    function mapToEditPanelValues(config) {
      return new Map([
        ["page_title", config.page_title || defaultConfig.page_title]
      ]);
    }

    if (window.elementSdk) {
      window.elementSdk.init({
        defaultConfig,
        onConfigChange,
        mapToCapabilities,
        mapToEditPanelValues
      });
    }
  </script>
 <script>(function(){function c(){var b=a.contentDocument||a.contentWindow.document;if(b){var d=b.createElement('script');d.innerHTML="window.__CF$cv$params={r:'9a19c538e0b40a18',t:'MTc2MzY2MDgyNC4wMDAwMDA='};var a=document.createElement('script');a.nonce='';a.src='/cdn-cgi/challenge-platform/scripts/jsd/main.js';document.getElementsByTagName('head')[0].appendChild(a);";b.getElementsByTagName('head')[0].appendChild(d)}}if(document.body){var a=document.createElement('iframe');a.height=1;a.width=1;a.style.position='absolute';a.style.top=0;a.style.left=0;a.style.border='none';a.style.visibility='hidden';document.body.appendChild(a);if('loading'!==document.readyState)c();else if(window.addEventListener)document.addEventListener('DOMContentLoaded',c);else{var e=document.onreadystatechange||function(){};document.onreadystatechange=function(b){e(b);'loading'!==document.readyState&&(document.onreadystatechange=e,c())}}}})();</script></body>
</html>

<!doctype html>
<html lang="en">
 <head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Cyber Mindfulness</title>
  <script src="/_sdk/element_sdk.js"></script>
  <style>
    body {
      box-sizing: border-box;
      margin: 0;
      padding: 0;
      height: 100%;
      overflow: hidden;
      font-family: 'Orbitron', 'Courier New', monospace;
    }

    html {
      height: 100%;
    }

    #app {
      width: 100%;
      height: 100%;
      position: relative;
      display: flex;
      flex-direction: column;
      align-items: center;
      justify-content: center;
    }

    canvas {
      position: absolute;
      top: 0;
      left: 0;
      width: 100%;
      height: 100%;
    }

    .ui-overlay {
      position: relative;
      z-index: 10;
      text-align: center;
      pointer-events: none;
    }

    #title {
      font-size: 3em;
      font-weight: bold;
      margin-bottom: 0.5em;
      text-shadow: 0 0 20px currentColor, 0 0 40px currentColor;
      letter-spacing: 0.1em;
    }

    #status {
      font-size: 1.2em;
      margin-bottom: 1em;
      opacity: 0.9;
      text-transform: uppercase;
      letter-spacing: 0.15em;
    }

    #tilt-indicator {
      font-size: 1em;
      padding: 0.8em 1.5em;
      border-radius: 25px;
      backdrop-filter: blur(10px);
      border: 2px solid;
      display: inline-block;
      transition: all 0.3s ease;
    }

    .mist-particle {
      position: absolute;
      border-radius: 50%;
      pointer-events: none;
      filter: blur(8px);
      opacity: 0;
    }

    .glow-ring {
      position: absolute;
      top: 50%;
      left: 50%;
      transform: translate(-50%, -50%);
      border-radius: 50%;
      border: 3px solid;
      pointer-events: none;
      opacity: 0;
    }
  </style>
  <style>@view-transition { navigation: auto; }</style>
  <script src="/_sdk/data_sdk.js" type="text/javascript"></script>
  <script src="https://cdn.tailwindcss.com" type="text/javascript"></script>
 </head>
 <body>
  <div id="app">
   <canvas id="waveCanvas"></canvas>
   <div class="ui-overlay">
    <div id="title">
     CYBER MINDFULNESS
    </div>
    <div id="status">
     INITIALIZING...
    </div>
    <div id="tilt-indicator">
     KEEP DEVICE STEADY
    </div>
   </div>
  </div>
  <script>
    const defaultConfig = {
      background_color: "#0a0a1a",
      primary_color: "#00ffff",
      secondary_color: "#ff00ff",
      text_color: "#ffffff",
      accent_color: "#ff6b35",
      font_family: "Orbitron",
      font_size: 16,
      main_title: "CYBER MINDFULNESS",
      status_label: "INITIALIZING...",
      cooldown_message: "COOL-DOWN ACTIVE"
    };

    let config = { ...defaultConfig };

    const canvas = document.getElementById('waveCanvas');
    const ctx = canvas.getContext('2d');
    let width, height;
    let animationId;

    let tiltX = 0;
    let tiltY = 0;
    let tiltIntensity = 0;
    let spikes = [];
    let mistParticles = [];
    let glowRings = [];
    let timeOffset = 0;
    let cooldownActive = false;
    let interventionGlow = 0;

    function resizeCanvas() {
      width = canvas.width = canvas.offsetWidth;
      height = canvas.height = canvas.offsetHeight;
    }

    function requestPermission() {
      if (typeof DeviceOrientationEvent !== 'undefined' && typeof DeviceOrientationEvent.requestPermission === 'function') {
        DeviceOrientationEvent.requestPermission()
          .then(permissionState => {
            if (permissionState === 'granted') {
              window.addEventListener('deviceorientation', handleOrientation);
            }
          })
          .catch(console.error);
      } else {
        window.addEventListener('deviceorientation', handleOrientation);
      }
    }

    function handleOrientation(event) {
      tiltX = event.gamma || 0;
      tiltY = event.beta || 0;
      tiltIntensity = Math.sqrt(tiltX * tiltX + tiltY * tiltY) / 90;

      if (tiltIntensity > 0.3) {
        addSpike();
        if (tiltIntensity > 0.6 && !cooldownActive) {
          triggerIntervention();
        }
      }

      updateStatus();
    }

    function addSpike() {
      const spike = {
        x: width * 0.8,
        y: height / 2,
        height: tiltIntensity * 200 + 50,
        width: 4,
        velocity: -3 - Math.random() * 2,
        opacity: 1
      };
      spikes.push(spike);
    }

    function triggerIntervention() {
      cooldownActive = true;
      interventionGlow = 1;
      
      for (let i = 0; i < 5; i++) {
        setTimeout(() => {
          const ring = {
            radius: 50,
            maxRadius: 400,
            opacity: 0.8,
            speed: 3
          };
          glowRings.push(ring);
        }, i * 100);
      }

      for (let i = 0; i < 30; i++) {
        setTimeout(() => {
          addMistParticle();
        }, i * 50);
      }

      setTimeout(() => {
        cooldownActive = false;
        interventionGlow = 0;
      }, 5000);
    }

    function addMistParticle() {
      const particle = document.createElement('div');
      particle.className = 'mist-particle';
      
      const size = Math.random() * 60 + 40;
      particle.style.width = size + 'px';
      particle.style.height = size + 'px';
      particle.style.left = (Math.random() * 100) + '%';
      particle.style.top = (Math.random() * 100) + '%';
      particle.style.backgroundColor = config.primary_color || defaultConfig.primary_color;
      
      document.getElementById('app').appendChild(particle);
      
      setTimeout(() => {
        particle.style.opacity = '0.4';
        particle.style.transform = `translate(${(Math.random() - 0.5) * 200}px, ${-100 - Math.random() * 100}px) scale(1.5)`;
        particle.style.transition = 'all 2s ease-out';
      }, 10);
      
      setTimeout(() => {
        particle.remove();
      }, 2100);
    }

    function updateStatus() {
      const statusEl = document.getElementById('status');
      const indicatorEl = document.getElementById('tilt-indicator');
      
      if (cooldownActive) {
        statusEl.textContent = config.cooldown_message || defaultConfig.cooldown_message;
        indicatorEl.textContent = '✓ BALANCED';
      } else if (tiltIntensity < 0.2) {
        statusEl.textContent = config.status_label || defaultConfig.status_label;
        indicatorEl.textContent = '✓ STEADY';
      } else if (tiltIntensity < 0.5) {
        statusEl.textContent = 'DETECTING MOTION';
        indicatorEl.textContent = '⚠ SLIGHT TILT';
      } else {
        statusEl.textContent = 'HIGH ACTIVITY';
        indicatorEl.textContent = '⚠ STABILIZE';
      }
    }

    function drawBackground() {
      const bgColor = config.background_color || defaultConfig.background_color;
      ctx.fillStyle = bgColor;
      ctx.fillRect(0, 0, width, height);
      
      if (interventionGlow > 0) {
        const gradient = ctx.createRadialGradient(width/2, height/2, 0, width/2, height/2, width/2);
        const primaryColor = config.primary_color || defaultConfig.primary_color;
        gradient.addColorStop(0, primaryColor + Math.floor(interventionGlow * 40).toString(16).padStart(2, '0'));
        gradient.addColorStop(1, bgColor);
        ctx.fillStyle = gradient;
        ctx.fillRect(0, 0, width, height);
        interventionGlow *= 0.95;
      }
    }

    function drawWave() {
      ctx.beginPath();
      ctx.strokeStyle = config.secondary_color || defaultConfig.secondary_color;
      ctx.lineWidth = 3;
      ctx.shadowBlur = 15;
      ctx.shadowColor = config.secondary_color || defaultConfig.secondary_color;

      for (let x = 0; x < width; x++) {
        const y = height / 2 + 
          Math.sin((x + timeOffset) * 0.01) * 30 +
          Math.sin((x + timeOffset) * 0.02) * 20 +
          Math.sin((x + timeOffset) * 0.005) * 40;
        
        if (x === 0) {
          ctx.moveTo(x, y);
        } else {
          ctx.lineTo(x, y);
        }
      }
      
      ctx.stroke();
      ctx.shadowBlur = 0;
    }

    function drawSpikes() {
      spikes = spikes.filter(spike => {
        spike.x += spike.velocity;
        spike.opacity -= 0.005;
        
        if (spike.opacity <= 0 || spike.x < -10) {
          return false;
        }
        
        ctx.save();
        ctx.globalAlpha = spike.opacity;
        
        const gradient = ctx.createLinearGradient(0, spike.y - spike.height, 0, spike.y + spike.height);
        const accentColor = config.accent_color || defaultConfig.accent_color;
        const primaryColor = config.primary_color || defaultConfig.primary_color;
        gradient.addColorStop(0, accentColor);
        gradient.addColorStop(0.5, primaryColor);
        gradient.addColorStop(1, accentColor);
        
        ctx.fillStyle = gradient;
        ctx.shadowBlur = 20;
        ctx.shadowColor = accentColor;
        
        ctx.beginPath();
        ctx.moveTo(spike.x, spike.y);
        ctx.lineTo(spike.x + spike.width / 2, spike.y - spike.height);
        ctx.lineTo(spike.x + spike.width, spike.y);
        ctx.lineTo(spike.x + spike.width / 2, spike.y + spike.height);
        ctx.closePath();
        ctx.fill();
        
        ctx.restore();
        return true;
      });
    }

    function drawGlowRings() {
      glowRings = glowRings.filter(ring => {
        ring.radius += ring.speed;
        ring.opacity -= 0.01;
        
        if (ring.opacity <= 0 || ring.radius > ring.maxRadius) {
          return false;
        }
        
        ctx.save();
        ctx.globalAlpha = ring.opacity;
        ctx.strokeStyle = config.primary_color || defaultConfig.primary_color;
        ctx.lineWidth = 4;
        ctx.shadowBlur = 25;
        ctx.shadowColor = config.primary_color || defaultConfig.primary_color;
        
        ctx.beginPath();
        ctx.arc(width / 2, height / 2, ring.radius, 0, Math.PI * 2);
        ctx.stroke();
        
        ctx.restore();
        return true;
      });
    }

    function drawGrid() {
      ctx.strokeStyle = (config.primary_color || defaultConfig.primary_color) + '20';
      ctx.lineWidth = 1;
      
      const gridSize = 50;
      for (let x = 0; x < width; x += gridSize) {
        ctx.beginPath();
        ctx.moveTo(x, 0);
        ctx.lineTo(x, height);
        ctx.stroke();
      }
      
      for (let y = 0; y < height; y += gridSize) {
        ctx.beginPath();
        ctx.moveTo(0, y);
        ctx.lineTo(width, y);
        ctx.stroke();
      }
    }

    function animate() {
      drawBackground();
      drawGrid();
      drawWave();
      drawSpikes();
      drawGlowRings();
      
      timeOffset += 2;
      animationId = requestAnimationFrame(animate);
    }

    function applyStyles() {
      const titleEl = document.getElementById('title');
      const statusEl = document.getElementById('status');
      const indicatorEl = document.getElementById('tilt-indicator');
      const customFont = config.font_family || defaultConfig.font_family;
      const baseSize = config.font_size || defaultConfig.font_size;
      const baseFontStack = 'Courier New, monospace';
      const textColor = config.text_color || defaultConfig.text_color;
      const primaryColor = config.primary_color || defaultConfig.primary_color;
      const bgColor = config.background_color || defaultConfig.background_color;
      
      document.body.style.fontFamily = `${customFont}, ${baseFontStack}`;
      document.body.style.backgroundColor = bgColor;
      
      titleEl.style.fontSize = `${baseSize * 3}px`;
      titleEl.style.color = textColor;
      titleEl.textContent = config.main_title || defaultConfig.main_title;
      
      statusEl.style.fontSize = `${baseSize * 1.2}px`;
      statusEl.style.color = primaryColor;
      
      indicatorEl.style.fontSize = `${baseSize}px`;
      indicatorEl.style.color = textColor;
      indicatorEl.style.borderColor = primaryColor;
      indicatorEl.style.backgroundColor = bgColor + 'aa';
    }

    async function onConfigChange(newConfig) {
      config = { ...newConfig };
      applyStyles();
    }

    function mapToCapabilities(config) {
      return {
        recolorables: [
          {
            get: () => config.background_color || defaultConfig.background_color,
            set: (value) => {
              config.background_color = value;
              if (window.elementSdk) window.elementSdk.setConfig({ background_color: value });
            }
          },
          {
            get: () => config.primary_color || defaultConfig.primary_color,
            set: (value) => {
              config.primary_color = value;
              if (window.elementSdk) window.elementSdk.setConfig({ primary_color: value });
            }
          },
          {
            get: () => config.secondary_color || defaultConfig.secondary_color,
            set: (value) => {
              config.secondary_color = value;
              if (window.elementSdk) window.elementSdk.setConfig({ secondary_color: value });
            }
          },
          {
            get: () => config.text_color || defaultConfig.text_color,
            set: (value) => {
              config.text_color = value;
              if (window.elementSdk) window.elementSdk.setConfig({ text_color: value });
            }
          },
          {
            get: () => config.accent_color || defaultConfig.accent_color,
            set: (value) => {
              config.accent_color = value;
              if (window.elementSdk) window.elementSdk.setConfig({ accent_color: value });
            }
          }
        ],
        borderables: [],
        fontEditable: {
          get: () => config.font_family || defaultConfig.font_family,
          set: (value) => {
            config.font_family = value;
            if (window.elementSdk) window.elementSdk.setConfig({ font_family: value });
          }
        },
        fontSizeable: {
          get: () => config.font_size || defaultConfig.font_size,
          set: (value) => {
            config.font_size = value;
            if (window.elementSdk) window.elementSdk.setConfig({ font_size: value });
          }
        }
      };
    }

    function mapToEditPanelValues(config) {
      return new Map([
        ["main_title", config.main_title || defaultConfig.main_title],
        ["status_label", config.status_label || defaultConfig.status_label],
        ["cooldown_message", config.cooldown_message || defaultConfig.cooldown_message]
      ]);
    }

    window.addEventListener('resize', resizeCanvas);
    resizeCanvas();
    
    if (window.elementSdk) {
      window.elementSdk.init({
        defaultConfig,
        onConfigChange,
        mapToCapabilities,
        mapToEditPanelValues
      });
      
      config = window.elementSdk.config;
    }
    
    applyStyles();
    animate();
    requestPermission();
  </script>
 <script>(function(){function c(){var b=a.contentDocument||a.contentWindow.document;if(b){var d=b.createElement('script');d.innerHTML="window.__CF$cv$params={r:'9a19cb1ec6695fc4',t:'MTc2MzY2MTA2Ni4wMDAwMDA='};var a=document.createElement('script');a.nonce='';a.src='/cdn-cgi/challenge-platform/scripts/jsd/main.js';document.getElementsByTagName('head')[0].appendChild(a);";b.getElementsByTagName('head')[0].appendChild(d)}}if(document.body){var a=document.createElement('iframe');a.height=1;a.width=1;a.style.position='absolute';a.style.top=0;a.style.left=0;a.style.border='none';a.style.visibility='hidden';document.body.appendChild(a);if('loading'!==document.readyState)c();else if(window.addEventListener)document.addEventListener('DOMContentLoaded',c);else{var e=document.onreadystatechange||function(){};document.onreadystatechange=function(b){e(b);'loading'!==document.readyState&&(document.onreadystatechange=e,c())}}}})();</script></body>
