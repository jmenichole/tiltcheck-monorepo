  <script>
    const defaultConfig = {
      main_heading: "TRUST BEHAVIOR",
      status_message: "you're doing great, recovery in progress ✨",
      boost_text: "+ACCOUNTABILITY",
      background_color: "#0d0520",
      accent_cyan: "#00ffff",
      accent_magenta: "#ff00ff",
      accent_purple: "#8a2be2",
      text_color: "#e0f7ff",
      font_family: "JetBrains Mono, Courier New",
      font_size: 16
    };

    async function onConfigChange(config) {
      const mainHeading = document.getElementById('mainHeading');
      const statusMessage = document.getElementById('statusMessage');
      const boostText = document.getElementById('boostText');

      if (mainHeading) mainHeading.textContent = config.main_heading || defaultConfig.main_heading;
      if (statusMessage) statusMessage.textContent = config.status_message || defaultConfig.status_message;
      if (boostText) boostText.textContent = config.boost_text || defaultConfig.boost_text;

      const bgColor = config.background_color || defaultConfig.background_color;
      const cyan = config.accent_cyan || defaultConfig.accent_cyan;
      const magenta = config.accent_magenta || defaultConfig.accent_magenta;
      const purple = config.accent_purple || defaultConfig.accent_purple;
      const textColor = config.text_color || defaultConfig.text_color;
      const fontFamily = config.font_family || defaultConfig.font_family;
      const fontSize = config.font_size || defaultConfig.font_size;

      document.body.style.background = `radial-gradient(ellipse at center, #1a0b2e 0%, ${bgColor} 100%)`;
      document.body.style.color = textColor;
      document.body.style.fontFamily = `${fontFamily}, monospace`;

      const sizeScale = fontSize / defaultConfig.font_size;
      
      if (mainHeading) {
        mainHeading.style.color = cyan;
        mainHeading.style.fontSize = `${2.5 * sizeScale}em`;
      }

      document.querySelectorAll('.panel-label').forEach(el => {
        el.style.color = cyan;
        el.style.fontSize = `${1.1 * sizeScale}em`;
      });

      document.querySelectorAll('.metric-display').forEach(el => {
        el.style.color = cyan;
        el.style.fontSize = `${3.5 * sizeScale}em`;
      });

      document.querySelectorAll('.behavior-curve').forEach(el => {
        el.style.stroke = cyan;
      });

      document.querySelectorAll('.tilt-spike').forEach(el => {
        el.style.stroke = magenta;
      });

      const topBar = document.querySelector('.top-bar');
      if (topBar) topBar.style.borderColor = purple;

      const graphPanel = document.querySelector('.graph-panel');
      if (graphPanel) graphPanel.style.borderColor = cyan;

      document.querySelectorAll('.metric-box').forEach(el => {
        el.style.borderColor = purple;
      });

      const tiltBar = document.querySelector('.tilt-bar');
      if (tiltBar) tiltBar.style.borderColor = magenta;

      const cooldownTimer = document.querySelector('.cooldown-timer');
      if (cooldownTimer) cooldownTimer.style.borderColor = cyan;

      const supportMessage = document.querySelector('.support-message');
      if (supportMessage) supportMessage.style.borderColor = magenta;
    }

    function mapToCapabilities(config) {
      return {
        recolorables: [
          {
            get: () => config.background_color || defaultConfig.background_color,
            set: (value) => {
              if (window.elementSdk) {
                window.elementSdk.config.background_color = value;
                window.elementSdk.setConfig({ background_color: value });
              }
            }
          },
          {
            get: () => config.accent_cyan || defaultConfig.accent_cyan,
            set: (value) => {
              if (window.elementSdk) {
                window.elementSdk.config.accent_cyan = value;
                window.elementSdk.setConfig({ accent_cyan: value });
              }
            }
          },
          {
            get: () => config.accent_magenta || defaultConfig.accent_magenta,
            set: (value) => {
              if (window.elementSdk) {
                window.elementSdk.config.accent_magenta = value;
                window.elementSdk.setConfig({ accent_magenta: value });
              }
            }
          },
          {
            get: () => config.accent_purple || defaultConfig.accent_purple,
            set: (value) => {
              if (window.elementSdk) {
                window.elementSdk.config.accent_purple = value;
                window.elementSdk.setConfig({ accent_purple: value });
              }
            }
          },
          {
            get: () => config.text_color || defaultConfig.text_color,
            set: (value) => {
              if (window.elementSdk) {
                window.elementSdk.config.text_color = value;
                window.elementSdk.setConfig({ text_color: value });
              }
            }
          }
        ],
        borderables: [],
        fontEditable: {
          get: () => config.font_family || defaultConfig.font_family,
          set: (value) => {
            if (window.elementSdk) {
              window.elementSdk.config.font_family = value;
              window.elementSdk.setConfig({ font_family: value });
            }
          }
        },
        fontSizeable: {
          get: () => config.font_size || defaultConfig.font_size,
          set: (value) => {
            if (window.elementSdk) {
              window.elementSdk.config.font_size = value;
              window.elementSdk.setConfig({ font_size: value });
            }
          }
        }
      };
    }

    function mapToEditPanelValues(config) {
      return new Map([
        ["main_heading", config.main_heading || defaultConfig.main_heading],
        ["status_message", config.status_message || defaultConfig.status_message],
        ["boost_text", config.boost_text || defaultConfig.boost_text]
      ]);
    }

    if (window.elementSdk) {
      window.elementSdk.init({
        defaultConfig,
        onConfigChange,
        mapToCapabilities,
        mapToEditPanelValues
      });
    }

    // Dynamic animations
    let trustValue = 82;
    setInterval(() => {
      trustValue = 70 + Math.floor(Math.random() * 25);
      document.getElementById('trustIndex').textContent = trustValue;
    }, 3500);

    let tiltValue = 28;
    setInterval(() => {
      tiltValue = 15 + Math.floor(Math.random() * 50);
      document.getElementById('tiltPercent').textContent = tiltValue + '%';
      document.getElementById('tiltBarFill').style.width = tiltValue + '%';
    }, 4200);

    let minutes = 4;
    let seconds = 32;
    setInterval(() => {
      seconds--;
      if (seconds < 0) {
        seconds = 59;
        minutes--;
        if (minutes < 0) minutes = 5;
      }
      const secStr = seconds < 10 ? '0' + seconds : seconds;
      document.getElementById('cooldownValue').textContent = `${minutes}:${secStr}`;
    }, 1000);
  </script>
