  <script>
    const defaultConfig = {
      brand_name: "TILTCHECK",
      background_color: "#0d0d1a",
      cyan_glow: "#00ffff",
      purple_shadow: "#9d4edd"
    };

    async function onConfigChange(config) {
      const brandName = config.brand_name || defaultConfig.brand_name;
      const backgroundColor = config.background_color || defaultConfig.background_color;
      const cyanGlow = config.cyan_glow || defaultConfig.cyan_glow;
      const purpleShadow = config.purple_shadow || defaultConfig.purple_shadow;

      // Update brand name
      const brandElement = document.getElementById('brandText');
      if (brandElement) {
        brandElement.textContent = brandName;
        brandElement.style.color = cyanGlow;
        brandElement.style.textShadow = `0 0 10px ${cyanGlow}, 0 0 20px ${cyanGlow}88, 0 0 30px ${cyanGlow}44`;
      }

      // Update background
      document.body.style.background = backgroundColor;

      // Update SVG colors
      const svg = document.querySelector('.neon-logo');
      if (svg) {
        // Cyan strokes
        const cyanPaths = svg.querySelectorAll('path[stroke="#00ffff"], line[stroke="#00ffff"]');
        cyanPaths.forEach(el => el.setAttribute('stroke', cyanGlow));

        // Cyan fills
        const cyanCircles = svg.querySelectorAll('circle[fill="#00ffff"]');
        cyanCircles.forEach(el => el.setAttribute('fill', cyanGlow));

        // Purple strokes
        const purplePaths = svg.querySelectorAll('[stroke="#9d4edd"]');
        purplePaths.forEach(el => el.setAttribute('stroke', purpleShadow));
      }
    }

    function mapToCapabilities(config) {
      return {
        recolorables: [
          {
            get: () => config.background_color || defaultConfig.background_color,
            set: (value) => {
              config.background_color = value;
              window.elementSdk.setConfig({ background_color: value });
            }
          },
          {
            get: () => config.cyan_glow || defaultConfig.cyan_glow,
            set: (value) => {
              config.cyan_glow = value;
              window.elementSdk.setConfig({ cyan_glow: value });
            }
          },
          {
            get: () => config.purple_shadow || defaultConfig.purple_shadow,
            set: (value) => {
              config.purple_shadow = value;
              window.elementSdk.setConfig({ purple_shadow: value });
            }
          }
        ],
        borderables: [],
        fontEditable: undefined,
        fontSizeable: undefined
      };
    }

    function mapToEditPanelValues(config) {
      return new Map([
        ["brand_name", config.brand_name || defaultConfig.brand_name]
      ]);
    }

    // Initialize SDK
    if (window.elementSdk) {
      window.elementSdk.init({
        defaultConfig,
        onConfigChange,
        mapToCapabilities,
        mapToEditPanelValues
      });
    }
  </script>
