        </lineargradient>
       </defs> <!-- Grid lines --> <line x1="0" y1="62.5" x2="500" y2="62.5" stroke="rgba(0,255,255,0.1)" stroke-width="1" /> <line x1="0" y1="125" x2="500" y2="125" stroke="rgba(0,255,255,0.1)" stroke-width="1" /> <line x1="0" y1="187.5" x2="500" y2="187.5" stroke="rgba(0,255,255,0.1)" stroke-width="1" /> <!-- Cooldown curve (background) --> <path class="cooldown-curve" d="M0,200 Q50,160 100,150 T200,140 T300,145 T400,135 T500,130 L500,250 L0,250 Z" /> <!-- Main behavior curve --> <path class="behavior-curve" d="M0,180 Q50,150 100,140 T200,135 T300,145 T400,130 T500,125">
        <animate attributename="d" dur="8s" repeatcount="indefinite" values="M0,180 Q50,150 100,140 T200,135 T300,145 T400,130 T500,125;
                        M0,185 Q50,155 100,145 T200,140 T300,150 T400,135 T500,130;
