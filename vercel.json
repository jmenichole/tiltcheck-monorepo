{
  "version": 2,
  "buildCommand": "cd frontend && npm install && npm run build",
  "installCommand": "npm install --legacy-peer-deps",
  "outputDirectory": "frontend/public",
  "framework": null,
  "env": {
    "NODE_ENV": "production",
    "NEXT_PUBLIC_API_URL": "@backend-api-url",
    "NEXT_PUBLIC_SUPABASE_URL": "@supabase-url",
    "NEXT_PUBLIC_SUPABASE_ANON_KEY": "@supabase-anon-key"
  },
  "functions": {
    "frontend/server.js": {
      "memory": 512,
      "maxDuration": 10
    }
  },
  "routes": [
    {
      "src": "^/api/(.*)",
      "dest": "https://api.tiltcheck.com/api/$1",
      "headers": {
        "Access-Control-Allow-Origin": "*",
        "Access-Control-Allow-Methods": "GET, POST, PUT, DELETE, OPTIONS",
        "Access-Control-Allow-Headers": "Content-Type, Authorization"
      }
    },
    {
      "src": "/styles/(.*)",
      "dest": "/styles/$1",
      "headers": {
        "Cache-Control": "public, max-age=31536000, immutable"
      }
    },
    {
      "src": "/js/(.*)",
      "dest": "/js/$1",
      "headers": {
        "Cache-Control": "public, max-age=3600"
      }
    },
    {
      "src": "/assets/(.*)",
      "dest": "/assets/$1",
      "headers": {
        "Cache-Control": "public, max-age=31536000, immutable"
      }
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        },
        {
          "key": "Referrer-Policy",
          "value": "strict-origin-when-cross-origin"
        },
        {
          "key": "Permissions-Policy",
          "value": "geolocation=(), microphone=(), camera=()"
        }
      ]
    }
  ],
  "regions": ["sfo1"],
  "public": true
}
      ]
    },
    {
      "source": "/(.*)",
      "headers": [
        { "key": "X-Frame-Options", "value": "DENY" },
        { "key": "X-Content-Type-Options", "value": "nosniff" },
        { "key": "X-XSS-Protection", "value": "1; mode=block" },
        { "key": "Referrer-Policy", "value": "strict-origin-when-cross-origin" }
      ]
    }
  ],
  "redirects": [
    {
      "source": "/invite",
      "destination": "https://discord.com/oauth2/authorize?client_id=1445916179163250860",
      "permanent": false
    },
    {
      "source": "/discord",
      "destination": "https://discord.gg/s6NNfPHxMS",
      "permanent": false
    },
    {
      "source": "/bot",
      "destination": "https://discord.com/oauth2/authorize?client_id=1445916179163250860",
      "permanent": false
    }
  ],
  "env": {
    "VERCEL_AI_GATEWAY_ENABLED": "true",
    "AI_GATEWAY_URL": "https://gateway.ai.cloudflare.com/v1"
  }
}
