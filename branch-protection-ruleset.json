{
  "name": "Protect main branch",
  "target": "branch",
  "enforcement": "active",
  "conditions": {
    "ref_name": {
      "exclude": [],
      "include": [
        "refs/heads/main"
      ]
    }
  },
  "rules": [
    {
      "type": "required_status_checks",
      "parameters": {
        "strict_required_status_checks_policy": false,
        "required_status_checks": [
          {
            "context": "components-a11y"
          },
          {
            "context": "landing-a11y"
          }
        ]
      }
    },
    {
      "type": "pull_request_required",
      "parameters": {}
    },
    {
      "type": "deletion",
      "parameters": {}
    },
    {
      "type": "required_linear_history",
      "parameters": {}
    },
    {
      "type": "required_signatures",
      "parameters": {}
    }
  ],
  "bypass_actors": []
}
